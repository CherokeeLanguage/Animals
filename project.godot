} ]
_global_script_class_icons={
"Main": "",
"Scenes": "",
"SmartAudioStream": "",
"Transition": "",
"UI_Button": "",
"about_scene": ""
}

[application]

config/name="Animals"
config/description="Learn the Cherokee names for animals."
run/main_scene="res://scenes/main/main.tscn"
boot_splash/image="res://assets/splash.png"
boot_splash/bg_color=Color( 0.0862745, 0.0862745, 0.0862745, 1 )
config/icon="res://assets/sprites/icon512.png"
config/quit_on_go_back=false
config/version="6.0.0"

[autoload]

Game="*res://autoload/game.gd"
ChallengeData="*res://classes/ChallengeData.gd"
WrongAudio="*res://scenes/WrongAudio.tscn"
EffectAudio="*res://scenes/EffectAudio.tscn"

[compression]

formats/zstd/long_distance_matching=true

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=1280
window/size/test_height=720
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "res://addons/smart_audio_stream/plugin.cfg" )

[gui]

theme/use_hidpi=true
theme/custom="res://themes/main_theme.tres"

[importer_defaults]

mp3={
"loop": false,
"loop_offset": 0
}

